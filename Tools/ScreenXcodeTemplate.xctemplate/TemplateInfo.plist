<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>AllowedTypes</key>
	<array>
		<string>public.swift-source</string>
	</array>
	<key>Platforms</key>
	<array>
		<string>com.apple.platform.iphoneos</string>
	</array>
	<key>DefaultCompletionName</key>
	<string>SwiftMvvmModule</string>
	<key>Description</key>
	<string>Basic Swift MVVM module template. Creates View, ViewModel, Model, DataSource, &amp; UI.</string>
	<key>Kind</key>
	<string>Xcode.IDEKit.TextSubstitutionFileTemplateKind</string>
	<key>MainTemplateFile</key>
	<string>___FILEBASENAME___</string>
	<key>SortOrder</key>
	<integer>2</integer>
	<key>Options</key>
	<array>
		<dict>
			<key>Description</key>
			<string>Name of screen (feature) that you need create</string>
			<key>Default</key>
			<string></string>
			<key>Identifier</key>
			<string>feature_name</string>
			<key>Name</key>
			<string>Screen name</string>
			<key>Required</key>
			<string>YES</string>
			<key>Type</key>
			<string>text</string>
		</dict>
		<dict>
			<key>Default</key>
			<string>___VARIABLE_feature_name___</string>
			<key>Identifier</key>
			<string>productName</string>
			<key>RequiredOptions</key>
      		<dict>
        	<key>templateType</key>
        	<array>
				<string>Files</string>
        	</array>
      		</dict>
			<key>Type</key>
			<string>static</string>
		</dict>
		<dict>
			<key>Identifier</key>
			<string>templateType</string>
			<key>Required</key>
			<true/>
			<key>Name</key>
			<string>Template Folder Name</string>
			<key>Description</key>
			<string>The type of template to use</string>
			<key>Type</key>
			<string>popup</string>
			<key>Default</key>
			<string>Files</string>
			<key>Values</key>
			<array>
			    <string>Files</string>
			</array>
		</dict>
	</array>
	<key>Template Author</key>
	<string>Andrey Arzhannikov</string>
</dict>
</plist>
